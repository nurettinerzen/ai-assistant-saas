{
  "name": "ai-assistant-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --env-file=.env --watch src/server.js",
    "build": "prisma generate",
    "postinstall": "prisma generate",
    "migrate": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:status": "npx prisma migrate status",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio",
    "test-user": "node create-test-user.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules' jest tests/unit",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' jest tests/e2e",
    "test:pilot-regression": "node tests/pilot/run-regression.js",
    "seed-test-data": "node scripts/seed-test-data.js",
    "security-test": "node scripts/security-smoke-test.js",
    "functional-test": "node scripts/functional-test.js",
    "assistant-test": "npm run seed-test-data && TEST_LEVEL=gate node tests/runner/assistant-test.js",
    "assistant-test:extended": "npm run seed-test-data && TEST_LEVEL=extended node tests/runner/assistant-test.js",
    "assistant-test:full": "npm run seed-test-data && TEST_LEVEL=full node tests/runner/assistant-test.js",
    "crm-test": "node tests/runner/crm-test.js",
    "crm-test:verbose": "node tests/runner/crm-test.js --verbose",
    "test:all-smoke": "npm run security-test && npm run functional-test && npm run assistant-test",
    "golden-test": "node tests/runner/golden-test.js",
    "replay-test": "node tests/replay/replayRunner.js",
    "test:security": "node tests/security/test-child-safety.js && node tests/security/test-ssrf-protection.js",
    "cleanup-assistants": "node scripts/cleanup-test-assistants.js",
    "cleanup-assistants:delete": "node scripts/cleanup-test-assistants.js --delete"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.22.0",
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.5",
    "google-auth-library": "^10.5.0",
    "googleapis": "^133.0.0",
    "html-to-text": "^9.0.5",
    "iyzipay": "^2.0.64",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "openai": "^6.9.1",
    "pdf-parse": "^2.4.5",
    "pg": "^8.17.2",
    "redis": "^5.10.0",
    "resend": "^6.6.0",
    "soap": "^1.6.1",
    "stripe": "^19.3.1",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  }
}
